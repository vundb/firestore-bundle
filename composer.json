{
    "name": "vundb/firestore-bundle",
    "type": "symfony-bundle",
    "version": "0.1.0",
    "require": {
        "php": ">=8.0",
        "google/cloud-firestore": "^1.42",
        "symfony/framework-bundle": "^7.0",
        "symfony/uid": "^7.0",
        "symfony/property-access": "^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vundb\\FirestoreBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vundb\\FirestoreBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/maker-bundle": "^1.57"
    }
}